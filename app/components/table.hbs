<table class="file-table">
      <tr>
      {{#each @result.columns as |column|}}
            <th>{{column}}</th>
      {{/each}}
      </tr>
      {{#each @result.files as |file|}}
      <tr>
            
            {{#each-in file as |name value|}}
                  <td>
                  {{tohtml (highlight value @result.keyword)}}
                  </td>
            {{/each-in}}
      </tr>
      {{/each}}
</table>